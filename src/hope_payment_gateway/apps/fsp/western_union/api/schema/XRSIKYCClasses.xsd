<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xrsi="http://www.westernunion.com/kyc/schema/xrsi" xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" targetNamespace="http://www.westernunion.com/kyc/schema/xrsi"
	jxb:version="1.0">
	<xs:include schemaLocation="XRSIKYCBasicTypes.xsd" />
		<xs:complexType name="channel">
		<xs:sequence>
			<xs:element minOccurs="1" name="type" type="xrsi:channel_type"/>
			<xs:element minOccurs="1" name="name" type="xrsi:channel_name"/>
			<xs:element minOccurs="1" name="version" type="xrsi:channel_version"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="session_digest">
		<xs:sequence>			
			<xs:element minOccurs="0" name="transaction_digest">
			  <xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="0"/>
					<xs:maxLength value="200"/>
				</xs:restriction>
		     </xs:simpleType>	
		  </xs:element>		
		   <xs:element minOccurs="0" name="correlation_id">
			  <xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="0"/>
					<xs:maxLength value="50"/>
				</xs:restriction>
		     </xs:simpleType>	
		  </xs:element>	
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="foreign_remote_system">
		<xs:sequence>
			<xs:element minOccurs="1" name="identifier" type="xrsi:identifier" />
			<xs:element minOccurs="1" name="reference_no" type="xrsi:reference_no" />
			<xs:element minOccurs="1" name="counter_id" type="xrsi:counter_id" />
			<xs:element minOccurs="0" name="operator_id" type="xrsi:operator_id" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="system">
		<xs:sequence>
			<xs:element name="id">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation/>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="version">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ip_address" type="xrsi:ip_address"/>			
			<xs:element name="connector" type="xrsi:connector"/>
		</xs:sequence>
	</xs:complexType>
		<xs:complexType name="connector">
		<xs:sequence>
		<xs:element name="id" minOccurs="1">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="0"/>
					<xs:maxLength value="84"/>
				</xs:restriction>
			</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>	
	<xs:complexType name="channel_partner">
		<xs:sequence>
			<xs:element name="id" minOccurs="1">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation/>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="system" type="xrsi:system"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="fla_details">
		<xs:sequence>
			<xs:element minOccurs="0" name="fla_id" type="xrsi:user_id" />
			<xs:element minOccurs="0" name="fla_password" type="xrsi:user_password" />
			<xs:element minOccurs="0" name="fla_firstname" type="xrsi:user_firstname" />
			<xs:element minOccurs="0" name="fla_lastname" type="xrsi:user_lastname" />
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:complexType name="SDQA">
   		<xs:sequence>
            <xs:element name="sdq" type="xs:string" minOccurs="1"/>
            <xs:element name="sda" type="xs:string" minOccurs="1"/>
    	</xs:sequence>
   </xs:complexType>

  	<xs:complexType name="SDQAs">
		<xs:sequence>
            <xs:element type="xrsi:SDQA" name="SDQA" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
   	</xs:complexType>
	
	
	
	<xs:complexType name="image_details">
		<xs:sequence>
			<xs:element minOccurs="0" name="image" type="xrsi:image" />
				<xs:element minOccurs="0" maxOccurs="1" name="image_type">
					<xs:simpleType>
						 <xs:restriction base="xs:string">           			 
		           			 <xs:enumeration value="ID_DOCUMENT"/>
		          		  <xs:enumeration value="DIGITAL_SIGNATURE"/>
		          		  <xs:enumeration value="RECEIPT"/>
<!-- 				NEW ADDITION 07-05-2015		 -->
						  <xs:enumeration value="OTHER"/>
<!-- 				NEW ADDITION 04-21-2020 for DUT eKYC CHANGES-->						  
						  <xs:enumeration value="PAYMENT_VERIFICATION"/>
		        		</xs:restriction>
					</xs:simpleType>
				</xs:element>
			<xs:element name="total_images" maxOccurs="1"	minOccurs="0" type="xs:int" />
			<xs:element name="image_index" maxOccurs="1" minOccurs="0" type="xs:int" />
			<xs:element minOccurs="0" name="doc_control_number" type="xrsi:doc_ref_number" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="document_details">
		<xs:sequence>
		     <xs:element name="is_doc_upload_completed" minOccurs="0" maxOccurs="1" >
				<xs:simpleType>
				  <xs:restriction base="xs:string">           			 
           			 <xs:enumeration value="Y"/>
          		     <xs:enumeration value="N"/>
        		  </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="document_index" maxOccurs="1" minOccurs="0" type="xs:int" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="entity_function_details">
		<xs:sequence>
			<xs:element minOccurs="0" name="agent_language" type="xrsi:agent_language" />
			<xs:element minOccurs="1" maxOccurs="1" name="doc_owner">
				<xs:simpleType>
				 <xs:restriction base="xs:string">           			 
           			 <xs:enumeration value="FLA"/>
          		  <xs:enumeration value="CUSTOMER"/>
        		</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="doc_capture_type">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="OFFLINE" />
						<xs:enumeration value="SIGNATURE" />
<!-- 						<xs:enumeration value="CONSENT" />  for future use -->
						<xs:enumeration value="IDCAPTURE" />
						<xs:enumeration value="RECEIPT" />
	<!-- 				NEW ADDITION 07-05-2015		 -->	 					
						<xs:enumeration value="NON_ID_CAPTURE" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>	
		</xs:sequence>
	</xs:complexType>	
	<xs:complexType name="transaction_details">
        <xs:sequence>
         <xs:element name="transaction_name" minOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">                       
                        <xs:enumeration value="SEND"/>
                        <xs:enumeration value="RECEIVE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <!-- MTCN on Search Response for selected record.-->
            <xs:element name="mtcn" minOccurs="0" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="16"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
			<xs:element name="date" maxOccurs="1" minOccurs="0" type="xs:date" />            
        </xs:sequence>
    </xs:complexType>
   
    <xs:complexType name="device">
		<xs:sequence>
			<xs:element name="id" minOccurs="0">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation/>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="type" minOccurs="0" type="xrsi:device_type"/>
		</xs:sequence>
	</xs:complexType>
		<xs:complexType name="customer_details">
		<xs:sequence>
				<xs:element minOccurs="0" name="name" type="xrsi:general_name"/>
				<xs:element minOccurs="0" name="mywu_number" type="xrsi:pref_cust_account_number"/>
				<xs:element minOccurs="0" name="email" type="xrsi:email"/>
			    <xs:element minOccurs="0" name="contact_phone" type="xrsi:phone"/>
				<xs:element minOccurs="0" name="postal_code" type="xrsi:postal_code"/>
				<xs:element minOccurs="0" name="upload_reason" type="xrsi:upload_reason"/>
				<xs:element minOccurs="0" name="customer_acting_as" type="xrsi:customer_acting_as"/>
				<!-- NEW ADDITION 07-05-2015  -->
				<xs:element minOccurs="0" name="customer_type" type="xrsi:customer_type"/>
				<xs:element name="additional_data_1" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>	
				<xs:element name="additional_data_2" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_3" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_4" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_5" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_6" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_7" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_8" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_9" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_10" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_11" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_12" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_13" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_14" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>
				<xs:element name="additional_data_15" type="xrsi:additionaldata" minOccurs="0" maxOccurs="1"/>			
											
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="general_name">
		<xs:complexContent>
			<xs:extension base="xrsi:name_type_base">
				<xs:sequence>
					<xs:group minOccurs="0" ref="xrsi:name_group"/>				
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="name_type_base">
		<xs:attribute name="name_type" type="xrsi:name_type"/>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:group name="name_group">
		<xs:choice>
			<xs:group minOccurs="0" ref="xrsi:individual_name"/>
			<xs:group minOccurs="0" ref="xrsi:organization_name"/>
			<xs:group minOccurs="0" ref="xrsi:mexican_name"/>
		</xs:choice>
	</xs:group>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:group name="individual_name">
		<xs:sequence>
			<xs:element minOccurs="0" name="first_name" type="xrsi:first_name"/>
			<xs:element minOccurs="0" name="middle_name" type="xrsi:middle_name"/>
			<xs:element minOccurs="0" name="last_name" type="xrsi:last_name"/>
		</xs:sequence>
	</xs:group>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:group name="mexican_name">
		<xs:sequence>
			<xs:element minOccurs="0" name="given_name" type="xrsi:first_name"/>
			<xs:element minOccurs="0" name="paternal_name" type="xrsi:paternal_name"/>
			<xs:element minOccurs="0" name="maternal_name" type="xrsi:maternal_name"/>
		</xs:sequence>
	</xs:group>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:group name="organization_name">
		<xs:sequence>
			<xs:element minOccurs="0" name="business_name" type="xrsi:business_name"/>
			<xs:element minOccurs="0" name="attention" type="xrsi:attention"/>
		</xs:sequence>
	</xs:group>
    
	<!-- KYC Template Fields -->
	<xs:complexType name="country_request_details">
		<xs:sequence>
			<xs:element name="language_code" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="ctry_name" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="max_record_count" maxOccurs="1"	minOccurs="0" type="xs:int" />
			<xs:element name="page_index" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="country_reply_details">
		<xs:sequence>
			<xs:element name="countries" maxOccurs="1" minOccurs="1" type="xrsi:countries" />
			<xs:element name="records_count" maxOccurs="1" minOccurs="1" type="xs:int" />
			<xs:element name="more_flag" maxOccurs="1" minOccurs="1">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Y"/>
					<xs:enumeration value="N"/>
				</xs:restriction>
			</xs:simpleType>
			</xs:element>
			<xs:element name="page_index" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="countries">
		<xs:sequence>
			<xs:element name="country" maxOccurs="unbounded" minOccurs="1" type="xrsi:country_details" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="country_details">
		<xs:sequence>
			<xs:element name="ctry_iso_code" maxOccurs="1" minOccurs="1" type="xrsi:country_iso_code" />
			<xs:element name="name" maxOccurs="1" minOccurs="1" type="xs:string" />
			<xs:element name="supported_flags" maxOccurs="1" minOccurs="1" type="xrsi:supported_flags" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="supported_flags">
		<xs:sequence>
			<xs:element name="db_support" maxOccurs="1" minOccurs="0" type="xrsi:flag" />
			<xs:element name="postalcode_support" maxOccurs="1" minOccurs="0" type="xrsi:flag" />
			<xs:element name="state_support" maxOccurs="1" minOccurs="0" type="xrsi:flag" />
			<xs:element name="state_abbr_support" maxOccurs="1" minOccurs="0" type="xrsi:flag" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="city_request_details">
		<xs:sequence>
			<xs:element name="language_code" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="ctry_iso_code" maxOccurs="1" minOccurs="1" type="xrsi:country_iso_code" />
			<xs:element name="state_name" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="city_name" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="max_record_count" maxOccurs="1" minOccurs="0" type="xs:int" />
			<xs:element name="page_index" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="city_info">
		<xs:sequence>
			<xs:element name="ctry_code" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="state" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="city" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="postal_code" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="city_reply_details">
		<xs:sequence>
			<xs:element name="cities" maxOccurs="1" minOccurs="1" type="xrsi:cities" />
			<xs:element name="records_count" maxOccurs="1" minOccurs="1" type="xs:int" />
			<xs:element name="more_flag" maxOccurs="1" minOccurs="1">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Y"/>
					<xs:enumeration value="N"/>
				</xs:restriction>
			</xs:simpleType>
			</xs:element>
			<xs:element name="page_index" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="cities">
		<xs:sequence>
			<xs:element name="city" maxOccurs="unbounded" minOccurs="1" type="xrsi:city_details" />
		</xs:sequence>
	</xs:complexType>	
	
	<xs:complexType name="city_details">
		<xs:sequence>
			<xs:element name="name" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="name_utf8" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="postal_code" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="state_request_details">
		<xs:sequence>
			<xs:element name="language_code" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="ctry_iso_code" maxOccurs="1" minOccurs="1" type="xrsi:country_iso_code" />
			<xs:element name="state_name" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="max_record_count" maxOccurs="1" minOccurs="0" type="xs:int" />
			<xs:element name="page_index" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="state_reply_details">
		<xs:sequence>
			<xs:element name="states" maxOccurs="1" minOccurs="1" type="xrsi:states" />
			<xs:element name="records_count" maxOccurs="1" minOccurs="1" type="xs:int" />
			<xs:element name="more_flag" maxOccurs="1" minOccurs="1">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Y"/>
					<xs:enumeration value="N"/>
				</xs:restriction>
			</xs:simpleType>
			</xs:element>
			<xs:element name="page_index" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="states">
		<xs:sequence>
			<xs:element name="state" maxOccurs="unbounded" minOccurs="1" type="xrsi:state_info" />
		</xs:sequence>
	</xs:complexType>	
	
	<xs:complexType name="state_info">
		<xs:sequence>
			<xs:element name="name" maxOccurs="1" minOccurs="1" type="xs:string" />
			<xs:element name="name_utf8" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="abbr" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="abbr_utf8" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>	
	<xs:complexType name="postal_code_request_details">
		<xs:sequence>
			<xs:element name="language_code" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="ctry_iso_code" maxOccurs="1" minOccurs="1" type="xrsi:country_iso_code" />
			<xs:element name="postal_code" maxOccurs="1" minOccurs="1" type="xs:string" />
			<xs:element name="max_record_count" maxOccurs="1" minOccurs="0" type="xs:int" />
			<xs:element name="page_index" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="postal_code_reply_details">
		<xs:sequence>
			<xs:element name="postal_code_detail" maxOccurs="unbounded" minOccurs="1"
				type="xrsi:postal_code_detail" />
			<xs:element name="records_count" maxOccurs="1" minOccurs="1" type="xs:int" />	
			<xs:element name="more_flag" maxOccurs="1" minOccurs="1">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Y"/>
					<xs:enumeration value="N"/>
				</xs:restriction>
			</xs:simpleType>
			</xs:element>
			<xs:element name="page_index" maxOccurs="1" minOccurs="0" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="postal_code_detail">
		<xs:sequence>
			<xs:element name="state_name" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="state_name_utf8" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="state_abbr" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="state_abbr_utf8" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="city_name" maxOccurs="1" minOccurs="1" type="xs:string" />
			<xs:element name="city_name_utf8" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="postal_code" maxOccurs="1" minOccurs="1" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="postal_code_list_details">
		<xs:sequence>
			<xs:element name="language_code" maxOccurs="1" minOccurs="0" type="xs:string" />
			<xs:element name="ctry_iso_code" maxOccurs="1" minOccurs="1" type="xrsi:country_iso_code" />
			<xs:element name="state_name" maxOccurs="1" minOccurs="0" type="xs:string" />
