				    <jaxb:typesafeEnumMember value="false" name="FALSE" />
				    <jaxb:typesafeEnumMember value="0" name="ZERO" />
				    <jaxb:typesafeEnumMember value="1" name="ONE" />
				    <jaxb:typesafeEnumMember value="" name="EMPTY" />
				</jaxb:typesafeEnumClass>
